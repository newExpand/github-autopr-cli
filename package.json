{
  "name": "newexpand-autopr",
  "version": "0.1.4",
  "description": "GitHub PR Automation CLI Tool with AI",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newExpand/github-autopr-cli.git"
  },
  "bin": {
    "autopr": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run copy-i18n",
    "copy-i18n": "cp -r src/i18n/locales dist/i18n/",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "github",
    "cli",
    "pr",
    "automation",
    "ai",
    "auto-pr",
    "newexpand-autopr",
    "pull-request",
    "code-review",
    "git",
    "github-automation",
    "pr-automation",
    "ai-code-review",
    "github-cli",
    "git-automation",
    "pr-management",
    "github-tools"
  ],
  "author": "newExpandgit",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^16.4.7",
    "i18next": "^24.2.2",
    "inquirer": "^12.4.2",
    "lodash": "^4.17.21",
    "minimatch": "^10.0.1",
    "octokit": "^4.1.2",
    "openai": "^4.86.2",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/i18next": "^13.0.0",
    "@types/inquirer": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "typescript": "~5.5.0",
    "vitest": "^1.2.2"
  }
}
